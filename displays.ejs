<html>
    <a href='/laptop/supplierinterface'>Add New Record</a><br>
    <center>
        <table border='1'>
            <caption><h3>List Of Suppliers</h3></caption>
            <tr><th>SN</th><th>Supplier ID</th><th>Supplier Name</th><th>Date of Birth</th><th>Gender</th><th>Supplier Address</th><th>State</th><th>City<th>Mobile No.</th><th>Phone No.</th><th>Dealing</th><th>Picture</th>
        <th>Edit/Delete</th>
        </tr>
            <%
            data.map(function(item,index){ %>
                <tr>
                    <td><%=(index+1) %></td>
                    <td><%=item.supplierid %></td>
                    <td><%=item.suppliername %></td>
                    <td><%=item.dob %></td>
                    <td><%=item.gender %></td>
                    <td><%=item.supplieraddress %></td>
                    <td><%=item.sname %></td>
                    <td><%=item.cname %></td>
                    <td><%=item.suppliermob %></td>
                    <td><%=item.supplierphone %></td>
                    <td><%=item.dealing %></td>
                    <td><img src='/images/<%=item.picture %>' width='40' height="40"></td>
                    <td><a href='/laptop/displaybyid?sid=<%=item.supplierid %>'>Edit/Delete</a></td>
                 
    
                </tr>
                <%
            })
            %>
        
        </table>
    </center>
</html>